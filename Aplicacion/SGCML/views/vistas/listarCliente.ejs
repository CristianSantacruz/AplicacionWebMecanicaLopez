<div class="row">
  <div class="row">

    <% for(var i=0; i<clientes.length; i++){ %>
    <div class="col-sm-4">
      <h5 class="text-center"> <%=clientes[i].nombre%></h5>
      <h5> Cédula: <%=clientes[i].cedula%></h5>
      <h5> Teléfono: <%=clientes[i].telefono%></h5>
      <h5> Empresa: <%=clientes[i].empresa%></h5>

      <a style="color:white" href="/cliente/borrarCliente?id=<%=clientes[i].id%>">
        <button class="btn btn-block btn-danger">Borrar Cliente</button>
      </a>
      <br>
    </div>
    <%  } %>
  </div>

  <div class="col-sm-5"></div>
  <div class="col-sm-2">
    <a href="/administracion">
      <button class="btn btn-primary btn-lg">Regresar</button>
    </a>
  </div>
  <div class="col-sm-5"></div>
  <br>
</div>
<br>
